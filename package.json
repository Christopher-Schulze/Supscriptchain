{
  "name": "hardhat-project",
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.97.1",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.14",
    "@openzeppelin/hardhat-upgrades": "^3.0.0",
    "@playwright/test": "^1.53.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/node": "^24.0.7",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "dotenv": "^17.0.1",
    "eslint": "^9.30.0",
    "hardhat": "^2.22.4",
    "hardhat-coverage": "^0.9.19",
    "hardhat-gas-reporter": "^2.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "matchstick-as": "^0.6.0",
    "playwright": "^1.53.2",
    "prettier": "^3.6.2",
    "solc": "^0.8.26",
    "solhint": "^5.2.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.4.5",
    "zod": "^3.25.73"
  },
  "scripts": {
    "compile": "hardhat compile",
    "test": "HARDHAT_DISABLE_DOWNLOAD_COMPILERS=true hardhat test",
    "gas": "HARDHAT_DISABLE_DOWNLOAD_COMPILERS=true REPORT_GAS=true hardhat test",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint --ext .ts .",
    "format": "prettier --write .",
    "solhint": "solhint 'contracts/**/*.sol'",
    "typechain": "typechain --target ethers-v6 --out-dir typechain ./artifacts/**/*.json",
    "codegen": "graph codegen subgraph/subgraph.yaml",
    "prepare-subgraph": "npx --yes ts-node scripts/prepare-subgraph.ts",
    "build-subgraph": "npm run prepare-subgraph && graph build subgraph/subgraph.local.yaml",
    "prepare": "husky install",
    "subgraph-server": "ts-node scripts/subgraph-server.ts",
    "coverage": "hardhat coverage",
    "slither": "slither .",
    "test-subgraph": "graph test",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@chainlink/contracts": "^1.4.0",
    "@openzeppelin/contracts": "^4.9.3"
  },
  "lint-staged": {
    "*": "prettier --check"
  }
}
